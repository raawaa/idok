    "axios": "^1.13.1",
    "concurrently": "^9.2.1",
    "electron": "^36.3.1",
    "electron-builder": "^24.9.1",
    "electron-reloader": "^1.2.3",
    "jest": "^30.2.0",
    "jsdom": "^27.1.0",
    "nock": "^14.0.10",
    "nodemon": "^3.1.10",
    "standard-version": "^9.5.0"
  },
  "dependencies": {
    "cheerio": "^1.1.2",
    "cloudscraper": "^4.6.0",
    "eventemitter3": "^5.0.1",

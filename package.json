{
  "name": "idok",
  "version": "0.4.1",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon",
    "dev:verbose": "concurrently \"nodemon --watch src --ext js,json,html,css --exec 'echo \"ğŸ”„ é‡æ–°å¯åŠ¨Electron...\" && electron .'\" \"echo \"ğŸ”¥ çƒ­é‡è½½å·²å¯åŠ¨ï¼Œä¿®æ”¹æ–‡ä»¶å°†è‡ªåŠ¨é‡æ–°åŠ è½½\"",
    "test": "node scripts/test-with-proxy.js",
    "test:watch": "node scripts/test-with-proxy.js --watch",
    "test:coverage": "node scripts/test-with-proxy.js --coverage",
    "test:proxy": "node scripts/test-with-proxy.js",
    "test:no-proxy": "USE_SYSTEM_PROXY=false jest",
    "test:integration": "jest --config jest.integration.config.js",
    "test:integration:proxy": "USE_SYSTEM_PROXY=true jest --config jest.integration.config.js",
    "test:regression": "node scripts/run-regression-tests.js",
    "test:regression:verbose": "node scripts/run-regression-tests.js --verbose",
    "test:regression:jest": "jest --config jest.config.regression.js",
    "test:regression:jest:coverage": "jest --config jest.config.regression.js --coverage",
    "test:regression:single": "node scripts/run-regression-tests.js",
    "test:regression:dry-run": "node scripts/run-regression-tests.js --dry-run",
    "test:regression:update": "node scripts/run-regression-tests.js --update-baseline",
    "test:regression:auto-update": "node scripts/auto-update-baseline.js",
    "test:regression:auto-update:dry-run": "node scripts/auto-update-baseline.js --dry-run",
    "test:unit": "jest --config jest.config.js --testPathPattern=tests/unit",
    "test:unit:coverage": "jest --config jest.config.js --testPathPattern=tests/unit --coverage",
    "test:web-scraper": "jest --config jest.config.regression.js --testPathPattern=web-scraper",
    "test:web-scraper:coverage": "jest --config jest.config.regression.js --testPathPattern=web-scraper --coverage",
    "build": "electron-builder",
    "release": "standard-version",
